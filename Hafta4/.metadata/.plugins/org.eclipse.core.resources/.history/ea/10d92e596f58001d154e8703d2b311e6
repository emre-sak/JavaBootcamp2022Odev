package Kodlama.io.Devs.business.abstracts;

import java.util.List;

import Kodlama.io.Devs.entites.concretes.ProgrammingLanguage;

public interface ProgrammingLanguageService {
	List<ProgrammingLanguage> getAll();
	List<ProgrammingLanguage> getAdd(ProgrammingLanguage programmingLanguage) throws Exception;
	List<ProgrammingLanguage> getUpdate(ProgrammingLanguage programmingLanguage, int index) throws Exception;
	List<ProgrammingLanguage> getRemove(int id);
	ProgrammingLanguage getById(int id);
}
