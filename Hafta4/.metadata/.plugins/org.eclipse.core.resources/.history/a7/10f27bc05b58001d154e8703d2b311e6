public interface ProgrammingLanguageRepository {
	List<ProgrammingLanguage> getAll();
	ProgrammingLanguage getAdd(ProgrammingLanguage programmingLanguage, String name, int id);
	ProgrammingLanguage getUpdate();
	ProgrammingLanguage getRemove();
	ProgrammingLanguage getById();
	
}
