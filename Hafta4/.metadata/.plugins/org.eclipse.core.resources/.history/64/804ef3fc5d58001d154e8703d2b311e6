package Kodlama.io.Devs.dataAccess.abstracts;

import java.util.List;

import Kodlama.io.Devs.entites.concretes.ProgrammingLanguage;

public interface ProgrammingLanguageRepository {
	List<ProgrammingLanguage> getAll();
	List<ProgrammingLanguage> getAdd(ProgrammingLanguage programmingLanguage);
	List<ProgrammingLanguage> getUpdate(ProgrammingLanguage programmingLanguage, int index);
	List<ProgrammingLanguage> getRemove(int id);
	ProgrammingLanguage getById();
	
}
